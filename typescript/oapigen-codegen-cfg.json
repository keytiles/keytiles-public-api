{
  "$schema": "./node_modules/@openapitools/openapi-generator-cli/config.schema.json",
  "spaces": 2,
  "generator-cli": {
    "version": "7.14.0",
    "generators": {

        "common-types-v2": {
            "inputSpec": "../src/main/openapi/common-types-v2.yaml",
            "output": "./src",
            "generatorName": "typescript-node",
            "packageName": "model/generated/common/types-v2",
            "modelPackage": "model/generated/common/types-v2",
            "apiPackage": "model/generated/common/types-v2",
            "globalProperty": {
                "apiDocs": false,
                "modelDocs": false,
                "apiTests": false,
                "modelTests": false
            }
        },

        "common-schedule-v1": {
            "inputSpec": "../src/main/openapi/common-schedule-v1.yaml",
            "output": "./src",
            "generatorName": "typescript-node",
            "packageName": "model/generated/common/schedule-v1",
            "modelPackage": "model/generated/common/schedule-v1",
            "apiPackage": "model/generated/common/schedule-v1",
            "globalProperty": {
                "apiDocs": false,
                "modelDocs": false,
                "apiTests": false,
                "modelTests": false
            }
        },

        "common-metadata-v1": {
            "inputSpec": "../src/main/openapi/common-metadata-v1.yaml",
            "output": "./src",
            "generatorName": "typescript-node",
            "packageName": "model/generated/common/metadata-v1",
            "modelPackage": "model/generated/common/metadata-v1",
            "apiPackage": "model/generated/common/metadata-v1",
            "globalProperty": {
                "apiDocs": false,
                "modelDocs": false,
                "apiTests": false,
                "modelTests": false
            }
        }

    }
  }
}
