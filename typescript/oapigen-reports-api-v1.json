{
  "$schema": "./node_modules/@openapitools/openapi-generator-cli/config.schema.json",
  "spaces": 2,
  "generator-cli": {
    "version": "7.14.0",
    "generators": {

        "report-api-v1": {
            "inputSpec": "../src/main/openapi/reports-api-v1.yaml",
            "output": "./src",
            "generatorName": "typescript-node",
            "packageName": "model/generated/reports-api-v1",
            "modelPackage": "model/generated/reports-api-v1",
            "apiPackage": "model/generated/reports-api-v1",
            "globalProperty": {
                "apiDocs": false,
                "modelDocs": false,
                "apiTests": false,
                "modelTests": false
            },
            "importMappings": {
                "BaseResponseClass": "../common/types-v2/baseResponseClass",
                "CommonErrorCodes": "../common/types-v2/commonErrorCodes",
                "ContainerQueryRangeResponseClass": "../common/types-v2/containerQueryRangeResponseClass",
                "ContainerResponseClass": "../common/types-v2/containerResponseClass",
                "MessageResponseClass": "../common/types-v2/messageResponseClass",
                "ProblemBaseClass": "../common/types-v2/problemBaseClass",
                "ProblemPlaceEnum": "../common/types-v2/problemPlaceEnum",
                "ResponseContainerInfoClass": "../common/types-v2/responseContainerInfoClass",

                "MetaData": "../common/metadata-v1/metaData",
                "ChangelogEntry": "../common/metadata-v1/changelogEntry",

                "ScheduleDayName": "../common/schedule-v1/scheduleDayName",
                "Schedule": "../common/schedule-v1/schedule",
                "HourlyScheduleSetup": "../common/schedule-v1/hourlyScheduleSetup",
                "DailyScheduleSetup": "../common/schedule-v1/dailyScheduleSetup",
                "WeeklyScheduleSetup": "../common/schedule-v1/weeklyScheduleSetup",
                "MonthlyScheduleSetup": "../common/schedule-v1/monthlyScheduleSetup"
            }
        }
    }
  }
}
